{
    "name": "simon/satslim2",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "require": {
        "cakephp/database": "^4",
        "monolog/monolog": "^2",
        "nyholm/psr7": "^1.4",
        "php-di/php-di": "*",
        "selective/basepath": "^2.1",
        "slim/psr7": "^1.5",
        "slim/slim": "4.*",
        "slim/php-view": "^3.0",
        "symfony/uid": "^6",
        "symfony/validator": "^6",
        "symfony/yaml": "^6"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.3",
        "selective/test-traits": "^2"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml --testdox",
        "test:coverage": "phpunit --configuration phpunit.xml --coverage-clover build/logs/clover.xml --coverage-html build/coverage"
    }
}
